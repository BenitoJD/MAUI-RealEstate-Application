<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealEstates.Pages.CustomTabbedPage"
             xmlns:local="clr-namespace:RealEstates.Pages"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             SelectedTabColor="AntiqueWhite"
             UnselectedTabColor="Aqua"
             Title="CustomTabbedPage">
    <NavigationPage Title="Home" IconImageSource="home_icon.png" BarBackground="#1d94ab" BarTextColor="White">
        <x:Arguments>
            <local:Home />
        </x:Arguments>
    </NavigationPage>
    <local:BookmarksPage Title="Saved" IconImageSource="bookmark_icon.png"/>
    <local:SettingsPage Title="Settings" IconImageSource="settings_icon.svg"/>
</TabbedPage>
